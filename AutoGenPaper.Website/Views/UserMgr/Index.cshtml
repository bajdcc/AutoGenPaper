@model IEnumerable<AutoGenPaper.Common.User>

@{
    ViewBag.Title = "用户管理";
}

<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
</hgroup>


<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RealName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RealName)
        </td>
        <td>
            @Html.AGPActionLink("删除", "Delete", new { id=item.UserId })
        </td>
    </tr>
}

</table>
